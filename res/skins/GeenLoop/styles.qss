	/* QCheckBox: Used at TreeView, for the music BPM */	
	QCheckBox::indicator {
    
    background:none;
	background-color:transparent;
	image:url(skin:graphics/library_checkbox.svg);
	outline:0px;
    }	
	QCheckBox::indicator:checked {
    background:none;
	background-color:transparent;
	image:url(skin:graphics/library_checkbox_checked.svg);
	}
	QCheckBox::indicator:unchecked {
    background:none;
	background-color:transparent;
	image:url(skin:graphics/library_checkbox.svg);
	}	
	
	/* WBeatSpinBox: Used at <BeatSpinBox> at Beat Jump and Beat Loop */	
	WBeatSpinBox {
	width:50px; min-width:50px; max-width:50px;	height:24px; min-height:24px; max-height:24px;
	background-color:#131313; color:#aaaaaa; border:1px solid #232323; /* border-radius:2px; - Rounded Corners */
	selection-background-color:#d2d2d2;	selection-color:#0f0f0f;
	font-size:11px/12px;
	padding:0px; padding-top:-3px; /* nedded to make the box centered with the buttons */
	margin:0px;	margin-bottom:3px; /* nedded to make the box centered with the buttons */
	}
	WBeatSpinBox:hover { border-color:#232323; }
	WBeatSpinBox:focus { border-color:#888888; color:#bbbbbb; }			
			
	/* QSpinBox: Used at 'Auto DJ - transition duration' */
	QSpinBox {
	border:1px solid #424242; color:#ffffff; background:transparent; margin:1px; padding:7px;
	selection-background-color:#d2d2d2;	selection-color:#0f0f0f;
	}
	QSpinBox::up-arrow { image:url(skin:graphics/library_arrow_up.svg); }
	QSpinBox::up-arrow:on { image:url(skin:graphics/library_arrow_up.svg); }
	QSpinBox::down-arrow { image:url(skin:graphics/library_arrow_down.svg); }
	QSpinBox::down-arrow:on { image:url(skin:graphics/library_arrow_down.svg); }
